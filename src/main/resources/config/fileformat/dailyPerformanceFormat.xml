<?xml version="1.0" encoding="UTF-8"?>
<FileFormat fileName="" fileType="xls" locale="US">
	<DataSet description="dailyPerformance">
	
		<DataSheet sheetName="" sheetIndex="0">
			<DataCell row="1" column="A" dataType="TEXT" fieldName="reportName"/>
			<DataCell row="4" column="C" dataType="TEXT" fieldName="listLotName" />
			<DataCell row="4" column="J" dataType="DATE" dataFormat="dd/MM/yyyy" fieldName="fromDate"/>
			<DataCell row="5" column="J" dataType="DATE" dataFormat="dd/MM/yyyy" fieldName="toDate"/>
